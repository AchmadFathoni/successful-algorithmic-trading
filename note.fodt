         <text:h text:style-name="P18" text:outline-level="3"><text:span text:style-name="T21">11.</text:span><text:span text:style-name="T22">2</text:span><text:span text:style-name="T21"> </text:span><text:span text:style-name="T22">Factor Choice</text:span></text:h>
        </text:list-header>
